{
  "map_enabled": false,
  "auto_pin_capturing_locations": false,
  "auto_unpin_on_clear": true,
  "allow_resize": false,
  "layout_swap_left_right": false,
  "disabled_image_filter": "grayscale, dim",
  "layouts": {
    "shared_item_grid": {
      "type": "itemgrid",
      "h_alignment": "center",
      "item_margin": "5,5",
      "item_size": 32,
      "rows": [
        [ "cubeofzoe", "spiritorb", "faeriescroll", "jewelofopen" ],
        [ "leapstone", "gravityboots", "mermanstatue", "holysymbol" ],
        [ "soulofbat", "fireofbat", "echoofbat", "forceofecho" ],
        [ "formofmist", "powerofmist", "gascloud" ],
        [ "soulofwolf", "powerofwolf", "skillofwolf" ],
        [ "faeriecard", "demoncard", "batcard", "ghostcard", "swordcard" ],
        [ "eyeofvlad", "ribofvlad", "heartofvlad", "toothofvlad", "ringofvlad" ]
      ]
    },
    "tracker_capture_item": {
      "type": "container",
      "content": {
        "type": "layout",
        "key": "shared_item_grid"
      }
    },
    "tracker_default": {
      "type": "container",
      "width": 360,
      //"height": 250,
      "background": "#212121",
      "content": {
        "type": "layout",
        "dropshadow": true,
        "margin": 10,
        "key": "shared_item_grid"
      }
    }
  }
}
